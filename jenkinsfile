pipeline {
	agent any
	
	stages {
		stage('Build') {
			steps {
				script {
                    bat 'msbuild WebApp.csproj /restore /p:Configuration=Release %MSBUILD_ARGS%'
                }
			}
		}
		stage('Publish') {
			steps {
				script {
                    bat 'dotnet publish -c Release -o ./publish'
                }
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...'
				sshagent(['nvanhecke']) {
					 echo 'In Key part'
                     bat 'scp.exe -r ./publish/* nvanhecke@4.211.249.95:/inetpub/wwwroot'
                }
               
			}
		}	
	 }
}