@model CityViewModel

<table class="table table-sm table-bordered text-white bg-@ViewBag.Theme">
    <thead>
        <tr><th colspan="2">Cities Summary</th></tr>
    </thead>
    <tbody>
        <tr>
            <th>Cities</th>
            <td>@Model.Cities</td>
        </tr>
        <tr>
            <th>Population</th>
            <td>@Model.Population</td>
        </tr>
    </tbody>
</table>