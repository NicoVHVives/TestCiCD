
@{
    Layout = "_SimpleLayout";
}

<table class="table table-striped table-bordered">
    <thead>
        <tr class="bg-primary text-white text-center">
            <th colspan="2">
                Form Data
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(string s in TempData.Keys)
        {
            <tr>
                <th>@s</th>
                <td>@TempData[s]</td>
            </tr>
        }
    </tbody>
</table>
<a class="btn btn-primary" asp-action="Index">Return</a>
